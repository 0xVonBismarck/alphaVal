{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "version": 2,
  "routes": [
    {
      "src": "/docs.html",
      "dest": "/docs.html"
    },
    {
      "src": "/alphaVal.md",
      "headers": {
        "Content-Type": "text/markdown",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 